    <BitmapImage x:Key="Mai" UriSource="/images/Mai.jpg"/>
    <BitmapImage x:Key="do-anh" UriSource="/images/do-anh.jpg"/>
    <BitmapImage x:Key="lat-mat-7" UriSource="/images/lat-mat-7.jpg"/>
    <BitmapImage x:Key="quy-cau" UriSource="/images/quy-cau.jpg"/>
